{
  "name": "sheetmusic-overviews",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark",
    "upd": "npm-check-updates -u",
    "upd:i": "npm-check-updates -u && npm i && npm audit fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@smui/button": "^6.0.0-beta.15",
    "@smui/card": "^6.0.0-beta.15",
    "@smui/checkbox": "^6.0.0-beta.15",
    "@smui/chips": "^6.0.0-beta.15",
    "@smui/common": "^6.0.0-beta.15",
    "@smui/dialog": "^6.0.0-beta.15",
    "@smui/form-field": "^6.0.0-beta.15",
    "@smui/icon-button": "^6.0.0-beta.15",
    "@smui/segmented-button": "^6.0.0-beta.15",
    "@smui/select": "^6.0.0-beta.15",
    "@smui/slider": "^6.0.0-beta.15",
    "@smui/top-app-bar": "^6.0.0-beta.15",
    "material-components-web": "^13.0.0",
    "npm-check-updates": "^12.5.3",
    "rollup": "^2.70.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^2.0.2",
    "smui-theme": "^6.0.0-beta.15",
    "standard": "^16.0.4",
    "svelte": "^3.46.4",
    "svelte-material-ui": "^6.0.0-beta.15",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@saehrimnir/druidjs": "^0.6.2",
    "d3": "^7.3.0",
    "jszip": "^3.7.1",
    "musicvis-lib": "^0.55.0",
    "opensheetmusicdisplay": "^1.4.5"
  }
}
