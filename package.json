{
  "name": "sheetmusic-overviews",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark",
    "upd": "npm-check-updates -u",
    "upd:i": "npm-check-updates -u && npm i && npm audit fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@smui/button": "^6.0.0",
    "@smui/card": "^6.0.0",
    "@smui/checkbox": "^6.0.0",
    "@smui/chips": "^6.0.0",
    "@smui/common": "^6.0.0",
    "@smui/dialog": "^6.0.0",
    "@smui/form-field": "^6.0.0",
    "@smui/icon-button": "^6.0.0",
    "@smui/segmented-button": "^6.0.0",
    "@smui/select": "^6.0.0",
    "@smui/slider": "^6.0.0",
    "@smui/top-app-bar": "^6.0.0",
    "material-components-web": "^14.0.0",
    "npm-check-updates": "^16.0.1",
    "rollup": "^2.77.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^2.0.2",
    "smui-theme": "^6.0.0",
    "standard": "^17.0.0",
    "svelte": "^3.49.0",
    "svelte-material-ui": "^6.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@saehrimnir/druidjs": "^0.6.2",
    "d3": "^7.6.1",
    "jszip": "^3.10.0",
    "musicvis-lib": "^0.55.0",
    "opensheetmusicdisplay": "^1.5.1"
  }
}
