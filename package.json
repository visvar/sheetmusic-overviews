{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark",
    "upd": "npm-check-updates -u",
    "upd:i": "npm-check-updates -u && npm i && npm audit fix"
  },
  "devDependencies": {
    "@material/animation": "^13.0.0",
    "@material/base": "^13.0.0",
    "@material/button": "^13.0.0",
    "@material/card": "^13.0.0",
    "@material/checkbox": "^13.0.0",
    "@material/chips": "^13.0.0",
    "@material/density": "^13.0.0",
    "@material/dialog": "^13.0.0",
    "@material/dom": "^13.0.0",
    "@material/elevation": "^13.0.0",
    "@material/feature-targeting": "^13.0.0",
    "@material/form-field": "^13.0.0",
    "@material/icon-button": "^13.0.0",
    "@material/ripple": "^13.0.0",
    "@material/rtl": "^13.0.0",
    "@material/segmented-button": "^13.0.0",
    "@material/shape": "^13.0.0",
    "@material/slider": "^13.0.0",
    "@material/theme": "^13.0.0",
    "@material/tokens": "^13.0.0",
    "@material/top-app-bar": "^13.0.0",
    "@material/touch-target": "^13.0.0",
    "@material/typography": "^13.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@smui/button": "^6.0.0-beta.6",
    "@smui/card": "^6.0.0-beta.6",
    "@smui/checkbox": "^6.0.0-beta.6",
    "@smui/chips": "^6.0.0-beta.6",
    "@smui/common": "^6.0.0-beta.6",
    "@smui/dialog": "^6.0.0-beta.6",
    "@smui/form-field": "^6.0.0-beta.6",
    "@smui/icon-button": "^6.0.0-beta.6",
    "@smui/segmented-button": "^6.0.0-beta.6",
    "@smui/select": "^6.0.0-beta.8",
    "@smui/slider": "^6.0.0-beta.6",
    "@smui/top-app-bar": "^6.0.0-beta.6",
    "npm-check-updates": "^12.0.3",
    "rollup": "^2.61.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.14",
    "smui-theme": "^6.0.0-beta.6",
    "standard": "^16.0.4",
    "svelte": "^3.44.2",
    "svelte-material-ui": "^6.0.0-beta.6"
  },
  "dependencies": {
    "@saehrimnir/druidjs": "^0.3.16",
    "d3": "^7.2.1",
    "jszip": "^3.7.1",
    "musicvis-lib": "^0.53.2",
    "opensheetmusicdisplay": "^1.3.1"
  }
}
